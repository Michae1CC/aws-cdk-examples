{
  "name": "coffee-app",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-p build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=./",
    "dev": "remix dev -c \"npm run dev:server\" --manual",
    "dev:server": "tsx watch --clear-screen=false --ignore 'app/**' --ignore 'build/**' --ignore 'node_modules/**' --inspect ./server.ts",
    "start": "cross-env NODE_ENV=production node server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@remix-run/css-bundle": "^2.5.1",
    "@remix-run/express": "^2.5.1",
    "@remix-run/node": "^2.5.1",
    "@remix-run/react": "^2.5.1",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "isbot": "^3.6.8",
    "markdown-it": "^14.0.0",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.5.1",
    "@remix-run/eslint-config": "^2.5.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/markdown-it": "^13.0.7",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@types/source-map-support": "^0.5.6",
    "autoprefixer": "^10.4.16",
    "chokidar": "^3.5.3",
    "esbuild": "^0.19.4",
    "eslint": "^8.38.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "ts-node": "^10.9.1",
    "tsx": "^3.14.0",
    "typescript": "^5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
